{
  "name": "read-books-online-backend",
  "version": "0.1.0",
  "scripts": {
    "start": "node            -r module-alias/register -r dotenv/config -r reflect-metadata                                    ../build/backend/src/index.js",
    "test:unit": "node        -r module-alias/register -r dotenv/config -r reflect-metadata -r ../build/backend/test/plugins.js ../build/backend/test/unit/index.js",
    "test:integration": "node -r module-alias/register -r dotenv/config                     -r ../build/backend/test/plugins.js ../build/backend/test/integration/index.js",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "delete-modules": "rimraf ./node_modules"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.1.3",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2",
    "yup": "^1.0.0-beta.1"
  },
  "devDependencies": {
    "@jest-mock/express": "^1.4.5",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mocha": "^9.0.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.11.7",
    "@types/sinon": "^10.0.6",
    "@types/uuid": "^8.3.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "fast-glob": "^3.2.10",
    "mocha": "^9.1.3",
    "mongodb-memory-server": "^8.1.0",
    "node-fetch": "^3.1.0",
    "read-books-online-core": "file:core",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1"
  }
}
