{
  "name": "read-books-online-backend",
  "version": "1.0.0",
  "dependencies": {
    "@typegoose/typegoose": "^9.3.1",
    "@types/uuid": "^8.3.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "module-alias": "^2.2.2",
    "mongoose": "^6.1.3",
    "reflect-metadata": "^0.1.13",
    "runtypes": "^6.5.0",
    "tsyringe": "^4.6.0",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "build": "npx tsc --build",
    "run": "node -r module-alias/register -r dotenv/config -r reflect-metadata build/backend/src/index.js",
    "watch": "npx tsc --watch",
    "prestart": "npm run build",
    "start": "npm run run",
    "poststart": "npm run clean"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.0"
  },
  "_moduleAliases": {
    "@": "build/backend/src",
    "@/common": "build/common"
  }
}
